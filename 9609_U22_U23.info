label FC93 >CMD
label FC9F <CMD
data FDE9-FE1A
code FE1B-FE20

word FDE7
word FDEB

remap    9F00-9FFF 4800 *to E700
remap    F84D-FC57 -9
remap    FC59-FC9E -C
remap    FCA0-FD01 1A
remap    FD1A      1A * TITLE
remap    FD24-FD2F 23
remap    FD30      38 * OK
remap    FD34      3A * BAD
remap    FD39-FD48 3B
remap    FD49-FDAB 3B
remap    FDAF-FB01 3B
remap    FDAF-FFD8 6F

* Use the 9619 info file which is very similar
include  9619_U7.info

setdp     F000 E7
setdp     F804-FF7F E7

uncomment E000-EFFF
uncomment FFE0-FFFF
uncomment 0000-F000
uncomment F800

unlabel  E000-E6FF
unlabel  E70F-E7FF
unlabel  FFDF-FFFF

label E3C0 ACAADDR0
label E3C8 ACAADDR1
label E3E0 TIMER

comment  F800 .
comment  F800     HARDWARE REQUIREMENTS:
comment  F800     THIS ROM AT $F800-$FFFF
comment  F800     RAM AT $E700-$E7FF
comment  F800     ACIA AT $E3C0
comment  F800     PTM AT $E3E0 (ENABLED TO NMI)
comment  F800 .

comment  FD1C Unclear why these JMPs are disabled. 
comment  FD1C Should be 9F (JMP indirect) as in CMS 9619.
